# Soul Order Online
# script for QuickBMS

get NAME string
get NAMESZ long
getdstring NAME NAMESZ
get DUMMY long
get OFFSET long
get DUMMY long
get FILES long
math OFFSET += 0x10
goto OFFSET
for i = 0 < FILES
    get NAMESZ long
    getdstring NAME NAMESZ
    get DUMMY byte
    get OFFSET long
    get ZSIZE long
    get SIZE long
    getdstring DUMMY 0x1c
    if SIZE == 0
        log NAME OFFSET ZSIZE
    else
        clog NAME OFFSET ZSIZE SIZE
    endif
next i
