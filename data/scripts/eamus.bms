get BNAME basename
string BNAME += "_"
set OFFSET 0

for i = 1
    goto OFFSET
    FindLoc OFFSET string "\x53\x43\x48\x6c\x2c\x0\x0\x0" 0 ""
    goto OFFSET
    FindLoc SIZE string "\x53\x43\x45\x6c\x8" 0 ""
    math SIZE += 5
    math SIZE -= OFFSET
    set NAME BNAME
    string NAME += i
    string NAME += ".asf"
    log NAME OFFSET SIZE
    math OFFSET += SIZE
next i