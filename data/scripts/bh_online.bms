# BH Online
# script for QuickBMS http://quickbms.aluigi.org

idstring "PRQS"
get VER long
get FILES long
get INFO_OFF long
get DUMMY long
goto INFO_OFF
for i = 0 < FILES
    get DUMMY1 long
    get OFFSET long
    get SIZE long
    get DUMMY4 long
    get DUMMY5 long
    filexor "0xB7 0xBD 0xD7 0xDB 0xB9 0xD8 0xD1 0xF7 0xA1 0xD2 0x62 0xFC 0x7A 0x60 0xCA 0xA4" OFFSET
    log "" OFFSET SIZE
    filexor ""
next i
