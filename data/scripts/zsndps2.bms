# ZSNDPS2 (script 0.1.1)

idstring "ZSNDPS2 "
get ZSD_SIZE long
get INFO_SIZE long
goto 0x30
get INFO_OFF long
get ZERO long
get INFO_SIZE long
goto INFO_OFF
for i = 0
    savepos INFO_OFF
    if INFO_OFF >= INFO_SIZE
        break
    endif
    get OFFSET long
    putarray 0 i OFFSET
next i
math FILES = i
putarray 0 i ZSD_SIZE
sortarray 0 1
for i = 0 < FILES
    getarray OFFSET 0 i
    math i + 1
    getarray SIZE   0 i
    math SIZE - OFFSET
    log "" OFFSET SIZE
next
