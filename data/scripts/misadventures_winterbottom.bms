get DUMMY long
get MAXOFF asize
for
    savepos MYOFF
    if MYOFF >= MAXOFF
        cleanexit
    endif

    get FILENUM long
    get SIZE long
    get PACKNUM long
    get OFFSET long
    get NAMESZ long
    getdstring NAME NAMESZ

    set PACKNAME string PACKNUM
    string PACKNAME += ".tock"
    open FDSE PACKNAME 1
    log NAME OFFSET SIZE 1
next
