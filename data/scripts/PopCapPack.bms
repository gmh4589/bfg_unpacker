# script for QuickBMS http://aluigi.org/papers.htm#quickbms

idstring 7½7M
filexor 0xf7

goto 9
savepos START
do
    get FNAMESZ byte
    getdstring FNAME FNAMESZ
    get SIZE long
    get SKIP long
    get SKIP long
    get LASTFILE byte
while LASTFILE != 0x80

savepos OFFSET
goto START
do
    get FNAMESZ byte
    getdstring FNAME FNAMESZ
    get SIZE long
    get SKIP long
    get SKIP long
    get LASTFILE byte

    log FNAME OFFSET SIZE
    math OFFSET += SIZE
while LASTFILE != 0x80
