# Quadra

idstring "UGS\0"
get ARCHIVE_SIZE asize
savepos OFFSET
for OFFSET = OFFSET != ARCHIVE_SIZE
    get NAMESZ long
    if NAMESZ == 0
        break
    endif
    getdstring NAME NAMESZ
    get SIZE long
    savepos OFFSET

    set EXT extension NAME
    if EXT == "rec"
        get XSIZE long
        math OFFSET + 4
        math SIZE   - 4
        clog NAME OFFSET SIZE XSIZE
    else
        log NAME OFFSET SIZE
    endif

    math OFFSET + SIZE
    goto OFFSET
next
