# Lifeline/Operator`s Side - *.a archive files

set OFFSET 0
idstring "arc\x00"
get DUMMY01 long
get FILES long
get BASE_OFF long
math OFFSET + BASE_OFF
for i = 0 < FILES
	getdstring NAME 0x10
	get SIZE1 long
	get DUMMY06 long
	get DUMMY07 long
	get DUMMY08 long
	get DUMMY09 long
	get SIZE2 long
	get DUMMY11 long
	get DUMMY12 long
	if SIZE2 == 0
		log NAME OFFSET SIZE1
	else
		log NAME OFFSET SIZE2
	endif
	math OFFSET += SIZE1
next i
