# Prison Life RPG

idstring "rac\x01"
get VER long    # 1
get INFO_SIZE long
get FILES long
for i = 0 < FILES
    get DUMMY long  # 1
    get OFFSET long
    get NAMESZ long
    math NAMESZ + 1
    getdstring NAME NAMESZ
    padding 4

    savepos TMP
    goto OFFSET
    get DUMMY long  # 2
    get XSIZE long  # aligned
    get SIZE long
    savepos OFFSET
    log NAME OFFSET SIZE
    goto TMP
next i
