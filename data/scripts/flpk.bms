# MicroMachines Xbox
# script for QuickBMS http://quickbms.aluigi.org

idstring FLPK
get VER long
get DUMMY long
get OFFSET long
goto 0x24
get FILES long
goto 0x30
for i = 0 < FILES
    get NAME_OFF long
    get SIZE long
    get OFFSET long
    get DUMMY long
    get DUMMY long
    math NAME_OFF += 0x20
    savepos TMP
    goto NAME_OFF
    get NAME string
    goto TMP
    log NAME OFFSET SIZE
next i
