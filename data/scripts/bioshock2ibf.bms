set MEMORY_FILE binary "\xff\xfe"
get LBF_SIZE asize
for OFFSET = 0 < LBF_SIZE

    get DATASZ variable2
    math DATASZ * 2
    getdstring DATA DATASZ
    set DATA unicode DATA
    set NAME string DATA

    get DATASZ variable2
    math DATASZ * 2
    savepos OFFSET

    log NAME 0 0
    append
    log NAME 0 2 MEMORY_FILE
    math DATASZ - 2
    log NAME OFFSET DATASZ
    math DATASZ + 2
    append

    math OFFSET + DATASZ
    goto OFFSET
next