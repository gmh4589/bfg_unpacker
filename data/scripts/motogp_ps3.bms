# MotoGP 13 PS3
# script for QuickBMS http://quickbms.aluigi.org

comtype deflate
endian big
get VER long
get FILES long
get OFFSET long
goto OFFSET
for i = 0 < FILES
    get NAMESZ long
    getdstring NAME NAMESZ
    get OFFSET long
    get ZSIZE long
    get SIZE long
    getdstring DUMMY 10
    clog "" OFFSET ZSIZE SIZE
next i
