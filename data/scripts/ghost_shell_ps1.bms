# Ghost in the Shell PS1

for i = 0
    get DUMMY byte
    get NAMESZ byte
    if NAMESZ == 0
        break
    endif
    getdstring NAME 0x12
    putvarchr NAME NAMESZ 0
    get OFFSET long
    putarray 0 i NAME
    putarray 1 i OFFSET
next i
    get OFFSET asize
    putarray 1 i OFFSET
math FILES = i
for i = 0 < FILES
    getarray NAME   0 i
    getarray OFFSET 1 i
    math i + 1
    getarray SIZE   1 i
    math SIZE - OFFSET
    log NAME OFFSET SIZE
next
