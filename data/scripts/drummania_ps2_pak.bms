open FDDE "pak" 0

get SIGN1 long
if SIGN1 == 0x704b4150
	get PAK_SIZE long
	get BASE_OFF long
	get FILES long
	goto 0x20
	get DUMMY03 long
	get DUMMY04 long
	get DUMMY05 long
	get DUMMY06 long
	get DUMMY07 long
	get DUMMY08 long
	get DUMMY09 long
	get DUMMY10 long
	math OFFSET + BASE_OFF
	for i = 0 < FILES
		get SIZE long
		get NAMESZ byte
		getdstring NAME NAMESZ
		padding 4
		log NAME OFFSET SIZE
		math OFFSET += SIZE
		putarray 0 i OFFSET
	next i
else
endif
