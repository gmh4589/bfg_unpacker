# Spider-Man: Friend or Foe DIR.BIN/DATA.BIN

get EXT extension
string TMP p "%s.%s" "DIR"  EXT
open FDSE TMP
string TMP p "%s.%s" "DATA" EXT
open FDSE TMP 1

get FILES asize
math FILES / 12
for i = 0 < FILES
    get NAME_CRC long
    get OFFSET long
    get SIZE long
    math OFFSET * 0x800
    string NAME p "%08x" NAME_CRC
    log NAME OFFSET SIZE 1
next i
