# Massive Assault Network (0.1.1)
# script for QuickBMS http://quickbms.aluigi.org

idstring XDAT
get DUMMY long
get DUMMY short
get VER short
get DUMMY long

set NAMESZ long 0x30
if VER > 3
    set NAMESZ long 0x40
endif

for
    getdstring NAME NAMESZ
    get TYPE long
    if TYPE == 0xffffffff
        cleanexit
    endif
    get OFFSET long
    get SIZE long

    log NAME OFFSET SIZE
next
