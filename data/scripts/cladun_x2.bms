# Cladun X2

open FDDE "tbl"
open FDDE "dat" 1
get FILES long
goto 0x10
for i = 0 < FILES
    get ID long
    get OFFSET long
    getdstring NAME 0x18
    putarray 0 i OFFSET
    putarray 1 i NAME
next i
    get OFFSET asize 1
    putarray 0 i OFFSET
for i = 0 < FILES
    getarray OFFSET 0 i
    getarray NAME   1 i
    math i + 1
    getarray SIZE   0 i
    math SIZE - OFFSET
    log NAME OFFSET SIZE 1
next i
