# extracts the GameData.fpk from various XBLA games
#
# supported games:
# - Flock!
#
# (c) 2012-04-04 by AlphaTwentyThree of XeNTaX

idstring "FKP"
get IDENT byte
goto 8
get FILES long
get UNK long
for i = 1 <= FILES
   savepos MYOFF
   getCT NAME string 0xcc
   math MYOFF += 0x30
   goto MYOFF
   get UNK long
   get UNK long
   get SIZE long
   get OFFSET long
   log NAME OFFSET SIZE
next i