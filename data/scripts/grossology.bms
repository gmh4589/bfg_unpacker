# Grossology RSC

idstring "GSRF"
get FILES long
for i = 0 < FILES
    getdstring NAME 0x10
    get SOME_CRC long
    get SIZE long
    get OFFSET long
    get XSIZE long
    if SIZE != XSIZE
        print "Error: SIZE %SIZE% != XSIZE %XSIZE%, contact me"
        cleanexit
    endif
    log NAME OFFSET SIZE
next i
