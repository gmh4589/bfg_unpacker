# Island Xtreme Stunts Quest decompressor

set MEMORY_FILE3 compressed ""

get NAME filename
get SIZE long
savepos OFFSET
get ZSIZE asize
math ZSIZE - OFFSET
if SIZE & 0x80000000
    math SIZE & 0x7fffffff
    log MEMORY_FILE OFFSET ZSIZE
    log MEMORY_FILE2 0 0
    putvarchr MEMORY_FILE2 SIZE 0
    calldll MEMORY_FILE3 0 cdecl RET MEMORY_FILE MEMORY_FILE2
    log NAME 0 SIZE MEMORY_FILE2
else
    log NAME OFFSET SIZE
endif
