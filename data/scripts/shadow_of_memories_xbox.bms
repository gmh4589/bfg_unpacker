# Shadow of Memories (XBOX) - game.wad/default.xbe

open FDSE "default.xbe"
open FDSE "game.wad" 1

get XBE_SIZE asize
goto 0x47d088
math TABLELENGTH = 0x5019a8
for CURR_OFF = 0 < TABLELENGTH
	getdstring NAME 0x40
	get SIZE long
	get OFFSET long
	get DUMMY3 long
	log NAME OFFSET SIZE 1
	savepos CURR_OFF
next
