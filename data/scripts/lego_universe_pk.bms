# LEGO Universe pk
# script for QuickBMS http://quickbms.aluigi.org

goto -8
get OFFSET longlong
goto OFFSET
get FILES long
for i = 0 < FILES
    get DUMMY1 long
    get DUMMY2 long
    get DUMMY3 long
    get SIZE long
    getdstring NAME 0x20
    get DUMMY5 long
    get DUMMY6 long
    getdstring NAME 0x20
    get DUMMY7 long
    get OFFSET long
    get DUMMY9 long
    log "" OFFSET SIZE
next i
