# ReyunSDK PAK

get ZERO long
get DUMMY long      # 0x012f5aad
get OFFSET long
get DATA_OFF long   # 0x10
goto OFFSET
get TMP asize
xmath FILES "(TMP - OFFSET) / (4+4+4+4+4+4)"
for i = 0 < FILES
    get NAME_CRC long
    get ZERO long
    get OFFSET long
    get ZSIZE long
    get SIZE long
    get ZERO long
    if SIZE != ZSIZE
        print "Error: SIZE != ZSIZE, contact me!"
        cleanexit
    endif
    log "" OFFSET SIZE
next i
