# Gran Turismo

getdstring SIGN 0xa
if SIGN == "@(#)GT-ARC"
	goto 0
	idstring "@(#)GT-ARC\x00\x00"
	get DUMMY1 short
	get FILES short
	for i = 0 < FILES	
		get OFFSET long
		get SIZE long
		get ZSIZE long
		if ZSIZE == SIZE
			log "" OFFSET SIZE
		else
			string TMPNAME p= "compressed/"
			log TMPNAME OFFSET SIZE
		endif
	next i
else
	break
endif
