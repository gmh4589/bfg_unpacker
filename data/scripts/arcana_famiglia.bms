# Arcana Famiglia (Hunex)

comtype lzss0

idstring "HFU2"
get FILES long
get BASE_OFF long
get DUMMY long  # 1
for i = 0 < FILES
    get OFFSET long
    get ZSIZE long
    get SIZE long
    get SOME_CRC long
    math OFFSET + BASE_OFF

    savepos TMP
    goto OFFSET
    idstring "UFFA"
    get ZSIZE long
    get SIZE long
    get ZERO long
    savepos OFFSET
    clog "" OFFSET ZSIZE SIZE
    goto TMP

next i

