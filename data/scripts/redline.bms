# Redline
# script for QuickBMS http://quickbms.aluigi.org

for EXTRACT = 0 < 2
    goto 0
    get VER long
    get FILES long
    for i = 0 < FILES
        get DUMMY long  # probably an obfuscated offset
        get SIZE long
        get NAMESZ long
        getdstring NAME NAMESZ
        if EXTRACT != 0
            log NAME OFFSET SIZE
            math OFFSET + SIZE
        endif
    next i
    savepos OFFSET
next EXTRACT
