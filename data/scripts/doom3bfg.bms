endian big
get DUMMY long
get INFOPOS long
goto INFOPOS
get FILES long
for i = 1 <= FILES
   get NAMEL long
   reverselong NAMEL
   getDstring NAME NAMEL
   get OFFSET long
   get SIZE long
   log NAME OFFSET SIZE
next i