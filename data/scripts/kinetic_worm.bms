# Kinetic Worm Dimension

idstring "RHF32"
goto 8
get FILES short
get FOLDERS short   # ???
get BASE_OFF long
get DUMMY long
for i = 0 < FILES
    get DUMMY long
    get OFFSET long
    get ZERO long
    get SIZE long
    getdstring NAME 0x20
    math OFFSET + BASE_OFF
    log NAME OFFSET SIZE
next i

