# script for QuickBMS #http://quickbms.aluigi.org

idstring \x63\x75\x62\x00\x31\x2E\x30\x00
get FILES long
goto 0x10c
for i = 1 <= FILES
   getDstring NAME 0x100
   get OFFSET long
   get SIZE long
   log NAME OFFSET SIZE
next i