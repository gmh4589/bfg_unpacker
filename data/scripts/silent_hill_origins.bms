# Silent Hill Origins
# script for QuickBMS http://quickbms.aluigi.org

idstring "A2.0"
get FILES long
get BASE_OFF long
get NAMES_OFF long
get NAMES_SIZE long
for i = 0 < FILES
    get NAME_OFF long
    get OFFSET long
    get ZSIZE long
    get SIZE long

    savepos TMP
    math NAME_OFF + NAMES_OFF
    goto NAME_OFF
    get NAME string
    goto TMP

    clog NAME OFFSET ZSIZE SIZE
next i
