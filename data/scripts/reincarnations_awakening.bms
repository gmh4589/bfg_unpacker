# Reincarnations Awakening
# script for QuickBMS http://quickbms.aluigi.org

comtype lzma
idstring "RESOURCE PACK\r\n"
get OFFSET long
goto OFFSET
get FILES asize
math FILES -= OFFSET
math FILES /= 0x21
for i = 0 < FILES
    getdstring NAME_HASH 20
    get OFFSET long
    get ZSIZE long
    get SIZE long
    get ZIP byte
    if ZIP == 0
        log "" OFFSET SIZE
    else
        math ZSIZE += 5
        clog "" OFFSET ZSIZE SIZE
    endif
next i
