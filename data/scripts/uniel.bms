# UNIEL - Melty Blood/Bread
# script for QuickBMS http://quickbms.aluigi.org

endian big
idstring "\x00CAP"
get ZERO long
get FILES long
get ZERO long
get BASE_OFF long
get NAMES_OFF long
get ZERO long
get ZERO long
for i = 0 < FILES
    get OFFSET long
    get SIZE long
    get NAME_OFF long
    get NAME_LEN long

    savepos TMP
    math NAME_OFF += NAMES_OFF
    goto NAME_OFF
    getdstring NAME NAME_LEN
    goto TMP

    math OFFSET += BASE_OFF
    log NAME OFFSET SIZE
next i
