# Battleship: Surface Thunder

idstring "MGFs"
get VER long
get MGF_SIZE long
get FILES long
get BASE_OFF long
get INFO_OFF long
goto INFO_OFF
for i = 0 < FILES
    get NAME_OFF long
    get SIZE long
    get DUMMY long
    get OFFSET long

    savepos TMP
    goto NAME_OFF
