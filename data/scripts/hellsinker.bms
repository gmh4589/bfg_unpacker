# Hellsinker.

math BASE_OFF = 0x203a4

goto 0x1f400
get FILES long
for i = 0 < FILES
    get SIZE long
    putarray 0 i SIZE
next i

goto 0x1fbd0
get ZERO long
for i = 0 < FILES
    get OFFSET long
    putarray 1 i OFFSET
next i

goto 0
for i = 0 < FILES
    get NAMESZ byte
    getdstring NAME NAMESZ
    padding 0x100

    getarray SIZE   0 i
    getarray OFFSET 1 i
    math OFFSET + BASE_OFF
    log NAME OFFSET SIZE
next i
