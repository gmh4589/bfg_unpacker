<bms games="'Empire 2'" platforms="'PC'" ext="DAT">
Set FILESTART Long 32 ;
Do ;
GoTo FILESTART 0 ;
GetDString FILENAME 6 0 ;
Get FILEOFF Long 0 ;
SavePos FILESTART 0 ;
GetDString FILENAME 6 0 ;
Get FILESIZE Long 0 ;
ReverseLong FILEOFF ;
ReverseLong FILESIZE ;
If FILESIZE = 0 ;
CleanExit ;
EndIf ;
Math FILESIZE -= FILEOFF ;
Log "" FILEOFF FILESIZE 0 0 ;
While NotEOF <> 0 ;
</bms>