# Spirit of Revenge - Vendel Games
# script for QuickBMS http://quickbms.aluigi.org

get EXT extension
if EXT != "vst"
    print "Error: this script works with the VST archive"
    cleanexit
endif

comtype unzip_dynamic

goto -4
get OFFSET long
goto OFFSET
get FILES long
for i = 0 < FILES
    get NAMESZ long
    getdstring NAME NAMESZ
    get OFFSET long
    get ZSIZE long
    clog NAME OFFSET ZSIZE ZSIZE
next i
