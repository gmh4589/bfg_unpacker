get FILES long
for i = 1 <= FILES
   get OFFSET long
   if OFFSET == 0
      cleanexit
   endif
   savepos MYOFF
   goto OFFSET
   get SIZE long
   get NAMEL long
   getDstring NAME NAMEL
   savepos OFFSET
   log NAME OFFSET SIZE
   goto MYOFF
next i