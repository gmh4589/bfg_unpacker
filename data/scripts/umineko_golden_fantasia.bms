# Umineko: Golden Fantasia
#   SE and BIN archives only
# script for QuickBMS http://quickbms.aluigi.org

get FILES long
if FILES == 0x524c5a4c
    print "Error: LZLR files not supported"
    cleanexit
endif
if FILES u> 0xffff
    print "Error: unsupported archive"
    cleanexit
endif
math FILES / 8
goto 0
for i = 0 < FILES
    get OFFSET long
    get SIZE long
    if SIZE != 0
        log "" OFFSET SIZE
    endif
next i
