open FDSE "DO.BIN"
for i = 0
    get OFFSET long
    if OFFSET == 0 && i != 0
        break
    endif
    putarray 0 i OFFSET
next i
math FILES = i
math FILES - 1
open FDSE "HAGECHO.BIN"
for i = 0 < FILES
    getarray OFFSET 0 i
    math i + 1
    getarray SIZE   0 i
    math SIZE - OFFSET
    log "" OFFSET SIZE
next
