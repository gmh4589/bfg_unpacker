# Beats of Rage
# script for QuickBMS http://quickbms.aluigi.org

idstring "PACK"
get VER long
goto -4
savepos INFO_SIZE
get INFO_OFF long
math INFO_SIZE -= INFO_OFF
goto INFO_OFF
for INFO_OFF = 0 < INFO_SIZE
    get NAMESZ long
    math INFO_OFF += NAMESZ
    get OFFSET long
    get SIZE long
    math NAMESZ -= 12
    getdstring NAME NAMESZ

    log NAME OFFSET SIZE
next
